{
    "rewrites": [
        {
            "source": "/api/wisphub/(.*)",
            "destination": "/api/wisphub"
        },
        {
            "source": "/api/wisphub",
            "destination": "/api/wisphub"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}